{
  "compilerOptions": {
    "jsx": "react",
    "lib": ["es6","dom"],
    "rootDir": "src",
    "outDir": "public/js",
    "sourceMap": true,
    "noImplicitAny": true,
    "module": "commonjs",
    "target": "es5",
    "moduleResolution": "node",
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "strictNullChecks": true
  },
  "include": [
    "./src"
  ],
  "exclude": [
    "./node_modules",
    "./server",
    "./public"
  ]
}